{
  "mcpVersion": "2024-11-05",
  "server": {
    "name": "lea-backend-turbo-boost",
    "version": "2.0.0",
    "description": "LEA Backend Turbo Boost - FastAPI project scaffolding + 66 UI components from 11 providers",
    "author": "beka4kaa",
    "homepage": "https://github.com/beka4kaa/lea",
    "repository": "https://github.com/beka4kaa/lea.git"
  },
  "tools": [
    {
      "name": "search_component",
      "description": "Search UI components using natural language queries across 66 components from 11 providers (MagicUI, Shadcn, DaisyUI, etc.)",
      "inputSchema": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "description": "Natural language search query (e.g., 'animated button', 'contact form', 'image gallery')"
          },
          "provider": {
            "type": "string",
            "enum": ["magicui", "shadcn", "daisyui", "reactbits", "tremor", "nextui", "chakra", "mantine", "antd", "arco", "semi"],
            "description": "Filter by specific provider"
          },
          "category": {
            "type": "string",
            "enum": ["animated", "forms", "navigation", "buttons", "inputs", "layouts", "data_display", "feedback", "overlays", "text", "backgrounds", "cards", "other"],
            "description": "Filter by component category"
          },
          "framework": {
            "type": "string",
            "enum": ["react", "vue", "svelte", "angular", "next", "nuxt"],
            "description": "Target framework compatibility"
          },
          "free_only": {
            "type": "boolean",
            "description": "Only return free components",
            "default": false
          },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 50,
            "default": 10,
            "description": "Number of results to return"
          }
        },
        "required": ["query"]
      }
    },
    {
      "name": "get_component_code",
      "description": "Get complete production-ready source code for any component with TypeScript interfaces, styling, and animations",
      "inputSchema": {
        "type": "object",
        "properties": {
          "component_id": {
            "type": "string",
            "pattern": "^[a-z]+/[a-z-]+$",
            "description": "Component ID in format 'provider/slug' (e.g., 'shadcn/button', 'magicui/contact-form')"
          },
          "format": {
            "type": "string",
            "enum": ["tsx", "jsx", "vue", "svelte", "html", "auto"],
            "default": "auto",
            "description": "Preferred code format"
          }
        },
        "required": ["component_id"]
      }
    },
    {
      "name": "list_components",
      "description": "List all available UI components with optional filtering by provider, category, framework, and pagination",
      "inputSchema": {
        "type": "object",
        "properties": {
          "provider": {
            "type": "string",
            "enum": ["magicui", "shadcn", "daisyui", "reactbits", "tremor", "nextui", "chakra", "mantine", "antd", "arco", "semi"],
            "description": "Filter by provider"
          },
          "category": {
            "type": "string",
            "enum": ["animated", "forms", "navigation", "buttons", "inputs", "layouts", "data_display", "feedback", "overlays", "text", "backgrounds", "cards", "other"],
            "description": "Filter by component category"
          },
          "framework": {
            "type": "string",
            "enum": ["react", "vue", "svelte", "angular", "next", "nuxt"],
            "description": "Filter by framework compatibility"
          },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 20,
            "description": "Number of components to return"
          },
          "offset": {
            "type": "integer",
            "minimum": 0,
            "default": 0,
            "description": "Number of components to skip for pagination"
          }
        }
      }
    },
    {
      "name": "get_component_docs",
      "description": "Get comprehensive documentation, usage examples, installation instructions, and dependencies for any component",
      "inputSchema": {
        "type": "object",
        "properties": {
          "component_id": {
            "type": "string",
            "pattern": "^[a-z]+/[a-z-]+$",
            "description": "Component ID in format 'provider/slug'"
          }
        },
        "required": ["component_id"]
      }
    },
    {
      "name": "get_block",
      "description": "Get complete UI blocks (multi-component layouts) like authentication pages, pricing sections, hero sections, navigation bars, footers, and dashboards",
      "inputSchema": {
        "type": "object",
        "properties": {
          "block_type": {
            "type": "string",
            "enum": ["auth", "pricing", "navbar", "hero", "footer", "dashboard", "landing"],
            "description": "Type of UI block to generate"
          },
          "target": {
            "type": "string",
            "enum": ["nextjs", "react", "vue", "svelte"],
            "default": "nextjs",
            "description": "Target framework"
          },
          "style": {
            "type": "string",
            "enum": ["tailwind", "css-modules", "styled-components"],
            "default": "tailwind",
            "description": "Styling approach"
          }
        },
        "required": ["block_type"]
      }
    },
    {
      "name": "install_plan",
      "description": "Generate complete installation plan with all dependencies, commands, and setup steps for selected components",
      "inputSchema": {
        "type": "object",
        "properties": {
          "component_ids": {
            "type": "array",
            "items": {"type": "string"},
            "minItems": 1,
            "description": "List of component IDs to install (e.g., ['shadcn/button', 'magicui/contact-form'])"
          },
          "target": {
            "type": "string",
            "enum": ["nextjs", "react", "vue", "svelte"],
            "default": "nextjs",
            "description": "Target framework"
          },
          "package_manager": {
            "type": "string",
            "enum": ["npm", "yarn", "pnpm", "bun"],
            "default": "npm",
            "description": "Package manager preference"
          }
        },
        "required": ["component_ids"]
      }
    },
    {
      "name": "verify",
      "description": "Verify component code for syntax errors, import issues, and framework compatibility",
      "inputSchema": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "description": "Component code to verify"
          },
          "framework": {
            "type": "string",
            "enum": ["react", "vue", "svelte", "nextjs"],
            "description": "Target framework for verification"
          },
          "check_imports": {
            "type": "boolean",
            "default": true,
            "description": "Check import statements for validity"
          },
          "check_syntax": {
            "type": "boolean",
            "default": true,
            "description": "Check syntax validity"
          }
        },
        "required": ["code", "framework"]
      }
    }
  ],
  "backend_tools": [
    {
      "name": "project_init",
      "description": "Initialize a complete FastAPI project with modern scaffolding, Docker, CI/CD, database, auth, and monitoring setup",
      "inputSchema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Project name (used for directory and package naming)"
          },
          "target_dir": {
            "type": "string",
            "description": "Target directory (defaults to current directory)"
          },
          "stack": {
            "type": "string",
            "enum": ["fastapi+uvicorn"],
            "default": "fastapi+uvicorn",
            "description": "Tech stack selection"
          },
          "db": {
            "type": "string",
            "enum": ["postgres", "sqlite"],
            "default": "postgres",
            "description": "Database type"
          },
          "orm": {
            "type": "string",
            "enum": ["sqlalchemy+alembic"],
            "default": "sqlalchemy+alembic",
            "description": "ORM and migration system"
          },
          "queue": {
            "type": "string",
            "enum": ["rq", "redis", "none"],
            "default": "rq",
            "description": "Background job queue system"
          },
          "docker": {
            "type": "boolean",
            "default": true,
            "description": "Include Docker and docker-compose setup"
          },
          "ci": {
            "type": "string",
            "enum": ["github", "gitlab", "none"],
            "default": "github",
            "description": "CI/CD system"
          },
          "telemetry": {
            "type": "boolean",
            "default": true,
            "description": "Include OpenTelemetry monitoring and Prometheus metrics"
          },
          "auth": {
            "type": "boolean",
            "default": true,
            "description": "Include JWT authentication scaffolding"
          },
          "preset": {
            "type": "string",
            "enum": ["api", "microservice", "full-stack"],
            "description": "Project preset configuration"
          }
        },
        "required": ["name"]
      }
    },
    {
      "name": "db_schema_design",
      "description": "Generate database models and schemas from specifications (Planned)",
      "status": "planned"
    },
    {
      "name": "api_crud_generate", 
      "description": "Generate CRUD API endpoints for entities (Planned)",
      "status": "planned"
    },
    {
      "name": "auth_enable",
      "description": "Enable complete authentication flows (Planned)",
      "status": "planned"
    },
    {
      "name": "deploy_preset",
      "description": "Configure deployment for Railway, Vercel, Docker, Kubernetes (Planned)",
      "status": "planned"
    }
  ],
  "examples": {
    "search_animated_button": {
      "tool": "search_component",
      "input": {"query": "animated button with hover effects", "limit": 5},
      "description": "Find animated button components"
    },
    "get_contact_form_code": {
      "tool": "get_component_code", 
      "input": {"component_id": "magicui/contact-form"},
      "description": "Get complete contact form code (4000+ characters)"
    },
    "list_magicui_components": {
      "tool": "list_components",
      "input": {"provider": "magicui", "limit": 10},
      "description": "List MagicUI components"
    },
    "get_pricing_block": {
      "tool": "get_block",
      "input": {"block_type": "pricing", "target": "nextjs"},
      "description": "Get complete pricing section"
    },
    "install_form_components": {
      "tool": "install_plan",
      "input": {"component_ids": ["magicui/contact-form", "shadcn/button"]},
      "description": "Get installation plan for form components"
    },
    "create_fastapi_project": {
      "tool": "project_init",
      "input": {
        "name": "my_awesome_api",
        "db": "postgres",
        "auth": true,
        "docker": true,
        "telemetry": true,
        "ci": "github"
      },
      "description": "Create a complete FastAPI project with PostgreSQL, auth, Docker, and GitHub Actions"
    },
    "create_microservice": {
      "tool": "project_init", 
      "input": {
        "name": "user_service",
        "preset": "microservice",
        "db": "postgres",
        "queue": "rq",
        "docker": true
      },
      "description": "Create a microservice with queue support"
    }
  },
  "capabilities": {
    "ui_components": {
      "total_components": 66,
      "providers": 11,
      "enhanced_features": [
        "Production-ready TSX code with TypeScript interfaces",
        "Interactive components (forms, modals, galleries, calculators)",
        "Enhanced template system for consistent code quality",
        "Framer Motion animations support",
        "Tailwind CSS v4 compatibility",
        "Multi-framework support (React, Vue, Svelte, Next.js)",
        "Comprehensive search and filtering",
        "Complete installation plans with dependencies"
      ]
    },
    "backend_generation": {
      "status": "TURBO BOOST MODE",
      "project_scaffolding": "Complete FastAPI project initialization",
      "templates": 17,
      "supported_stacks": ["fastapi+uvicorn"],
      "databases": ["PostgreSQL", "SQLite"],
      "features": [
        "Complete project structure generation",
        "SQLAlchemy + Alembic database setup",
        "JWT authentication scaffolding",
        "Docker multi-stage builds",
        "GitHub Actions CI/CD pipelines", 
        "OpenTelemetry monitoring integration",
        "Prometheus metrics collection",
        "Production-ready configurations",
        "Comprehensive documentation generation",
        "Environment-based settings management"
      ],
      "deployment_targets": ["Railway", "Docker", "Kubernetes"],
      "planned_tools": 22,
      "implemented_tools": 5
    }
  },
  "providers": {
    "magicui": {
      "components": 36,
      "enhanced_templates": true,
      "interactive_components": ["contact-form", "modal-dialog", "image-gallery", "loading-spinner", "tooltip", "calculator"],
      "description": "Animated components with enhanced templates and interactive elements"
    },
    "shadcn": {
      "components": 30,
      "enhanced_templates": true,
      "core_components": ["button", "input", "card", "badge", "avatar"],
      "description": "Core UI components with enhanced template system"
    },
    "daisyui": {
      "components": "varies",
      "description": "Semantic component classes for Tailwind CSS"
    }
  },
  "integration": {
    "protocol": "MCP 2024-11-05",
    "transport": "JSON-RPC 2.0",
    "endpoint": "/mcp",
    "discovery": "/mcp-discovery",
    "health": "/health",
    "documentation": "https://github.com/beka4kaa/lea/blob/main/MCP_AI_AGENT_GUIDE.md"
  }
}